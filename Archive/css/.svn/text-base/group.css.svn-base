  font-size: 14px;
  margin-left: 180px;
  margin-top: 5px;
}

div#content_div div.actionlinks a.highlightlink {
  background-color: #e5edfa;
}

div#group_profile_form div.form_section div.privacy input {
  margin-right: 10px;
  margin-top: 7px;
}
