td#left_col img.group_header {
  display: block;
}

td#center_col div.not_admin {
  font-size: 14px;
  font-weight: bold;
  font-style: italic;
  color: #666;
}

td#center_col div#group_manageadmins_header,
td#center_col div#group_transferownership_header
{
  margin-top: 20px;
}

